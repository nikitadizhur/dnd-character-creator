{
  "compilerOptions": {
    "target": "es6",
    "sourceMap": true,
    "outDir": "./dist/",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "jsx": "react",
    "noImplicitReturns": true,
    "types": ["cypress"]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ],
}
